{
    "manifest_version": 2,
    "version":"0.1",
    "name": "Hyperlinks For Stock Symbols",
    "content_scripts": [
      {
        "matches": [
            "<all_urls>"
        ],
        "css": [
            "css/content.css"
        ],
        "js": [
            "js/jquery-3.5.0.min.js", 
            "js/constants.js",
            "js/functions.js",
            "js/content.js"
        ]
      }
    ],
    "web_accessible_resources":
    [
      "data/temp.txt",
      "data/nasdaqlisted.txt",
      "data/otherlisted.txt"
    ]
  }